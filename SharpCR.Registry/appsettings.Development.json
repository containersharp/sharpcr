{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 20,
      "MaxRequestBodySize": 1073741824
    }
  }, 
  "Features": {
    "Toggles": {
      "CloudStorage": false
    },
    "SyncIntegration": {
      "SyncTimeoutSeconds": 300,
      "MirrorModeBaseDomain": "local",
      "Dispatcher": {
        "BaseUrl": "https://localhost:5003",
        "AuthorizationToken": "hello"
      }
    }
  }
}
